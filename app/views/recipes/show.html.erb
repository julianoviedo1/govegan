<div class="card m-3">
  <%=@recipe.name%>
  <iframe width="560" height="315" src=<%=@recipe.video_url%> frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
  <ul>
    <%@ingredients.each do |ingredient|%>
    <li><%=ingredient.name%>
    <%=ingredient.quantity%>
    <%=ingredient.unit%></li>
    <%end%>
  </ul>
</div>
